<Window x:Class="Demo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Demo"
        xmlns:wpfFx="clr-namespace:Demo.WpfFX"
        mc:Ignorable="d"
        Height="900" Width="1200"
        xmlns:ui="http://schemas.modernwpf.com/2019"
        Title="MvvmChart Demo (WPF/.NET Framework)" >


    <TabControl TabStripPlacement="Left" Margin="20">
        <TabItem Header="Category Axis Demo" Padding="6">
            <wpfFx:CategoryAxisDemoView Margin="15"/>
        </TabItem>

        <TabItem Header="General Demo" Padding="6">
            <local:GeneralDemoView Margin="15"/>
        </TabItem>

        <TabItem Header="Series Customization" Padding="6">
            <wpfFx:SeriesCustomizingDemoView Margin="15"/>
        </TabItem>

        <TabItem Header="Scatter Customization" Padding="6">
            <local:ScatterCustomizationDemoView Margin="15"/>
        </TabItem>

        <TabItem Header="DateTime Demo" Padding="6">
            <local:DateTimeDemoView Margin="15"/>
        </TabItem>

        <TabItem Header="Big Data Test" Padding="6">
            <local:BigDataTestView Margin="15"/>
        </TabItem>


        
        
    </TabControl>

    


</Window>
